{
  "name": "@remobj/shared",
  "version": "0.1.0",
  "description": "Shared utilities for RemObj library",
  "main": "dist/shared.umd.js",
  "module": "dist/shared.esm.js",
  "browser": "dist/shared.umd.js",
  "types": "dist/shared.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/shared.esm.js",
      "require": "./dist/shared.umd.js",
      "bundler": {
        "default": "./dist/shared.bundler.js",
        "source": "./dist/shared.bundler.js.map"
      },
      "types": "./dist/shared.d.ts"
    }
  },
  "files": [
    "dist",
    "dist/*.map"
  ],
  "sideEffects": false,
  "buildOptions": {
    "name": "RemObjShared",
    "formats": [
      "esm-production",
      "esm-bundler",
      "umd"
    ],
    "globalName": "RemObjShared",
    "external": []
  },
  "keywords": [
    "utility",
    "shared",
    "remobj"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remobj/remobj.git",
    "directory": "packages/shared"
  },
  "bugs": {
    "url": "https://github.com/remobj/remobj/issues"
  },
  "homepage": "https://github.com/remobj/remobj/tree/main/packages/shared#readme"
}
